{
  "name": "kdu-i18n",
  "version": "9.1.10-beta.0",
  "description": "Internationalization plugin for Kdu.js",
  "keywords": [
    "i18n",
    "internationalization",
    "plugin",
    "kdu",
    "kdu.js"
  ],
  "license": "MIT",
  "author": "NKDuy",
  "homepage": "https://github.com/khanhduy1407/kdu-i18n/tree/master/packages/kdu-i18n#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/khanhduy1407/kdu-i18n.git",
    "directory": "packages/kdu-i18n"
  },
  "bugs": {
    "url": "https://github.com/khanhduy1407/kdu-i18n/issues"
  },
  "files": [
    "index.js",
    "dist",
    "ketur"
  ],
  "main": "index.js",
  "module": "dist/kdu-i18n.esm-bundler.js",
