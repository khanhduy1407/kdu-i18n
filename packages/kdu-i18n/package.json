  "jsdelivr": "dist/kdu-i18n.global.js",
  "types": "dist/kdu-i18n.d.ts",
  "dependencies": {
    "@kdu-i18n/core": "9.1.10-beta.0",
    "@kdu-i18n/shared": "9.1.10-beta.0",
