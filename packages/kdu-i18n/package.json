    "@kdujs/devtools-api": "^6.0.0-beta.7"
  },
  "devDependencies": {
    "@kdu-i18n/devtools-if": "9.1.10-beta.0"
  },
  "peerDependencies": {
    "kdu": "^3.1.5-alpha.1"
  },
  "engines": {
    "node": ">= 10"
  },
  "buildOptions": {
    "name": "KduI18n",
    "formats": [
      "esm-bundler",
      "esm-bundler-runtime",
      "esm-browser",
      "esm-browser-runtime",
      "cjs",
      "global",
      "global-runtime"
